<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="tag">

    <sql id="search">
        <where>
            tag_code=#{keyword}
        </where>
    </sql>

    <select id="tag" resultType="Tag">
        SELECT * FROM tag
    </select>

    
</mapper>